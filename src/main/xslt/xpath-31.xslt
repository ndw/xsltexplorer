                                                                                         '>>' | '?' | '[' | ']' | 'and' | 'cast' | 'castable' | 'div' | 'else' |
                                                                                         'eq' | 'except' | 'ge' | 'gt' | 'idiv' | 'instance' | 'intersect' |
                                                                                         'is' | 'le' | 'lt' | 'mod' | 'ne' | 'or' | 'return' | 'satisfies' |
                                                                                         'to' | 'treat' | 'union' | '|' | '||' | '}' -->
        <xsl:choose>
          <xsl:when test="$state[$p:l1] != 16                                         (: '(' :)
                      and $state[$p:l1] != 39                                         (: '?' :)
                      and $state[$p:l1] != 41">                                     <!-- '[' -->
